<p id="notice"><%= notice %></p>

<h1>Listing Claims</h1>

<table>
  <thead>
    <tr>
      <th>Aid</th>
      <th>Type</th>
      <th>Rank</th>
      <th>Snaktype</th>
      <th>Property</th>
      <th>Value string</th>
      <th>Value item</th>
      <th>Value type</th>
      <th>Datatype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @claims.each do |claim| %>
      <tr>
        <td><%= claim.aid %></td>
        <td><%= claim.type %></td>
        <td><%= claim.rank %></td>
        <td><%= claim.snaktype %></td>
        <td><%= claim.property %></td>
        <td><%= claim.value_string %></td>
        <td><%= claim.value_item %></td>
        <td><%= claim.value_type %></td>
        <td><%= claim.datatype %></td>
        <td><%= link_to 'Show', claim %></td>
        <td><%= link_to 'Edit', edit_claim_path(claim) %></td>
        <td><%= link_to 'Destroy', claim, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Claim', new_claim_path %>
